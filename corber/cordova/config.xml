<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<widget id="io.corber.hybridappPart2" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>hybridappPart2</name>
    <description>
    </description>
    <content src="index.html"/>
    <plugin name="cordova-plugin-whitelist" spec="1"/>
    <plugin name="cordova-plugin-device-motion" spec="^2.0.1"/>
    <plugin name="cordova-plugin-vibration" spec="^3.1.0"/>
    <plugin name="cordova-plugin-device" spec="^2.0.2"/>
    <access origin="*"/>
    <allow-intent href="http://*/*"/>
    <allow-intent href="https://*/*"/>
    <allow-intent href="tel:*"/>
    <allow-intent href="sms:*"/>
    <allow-intent href="mailto:*"/>
    <allow-intent href="geo:*"/>
    <platform name="android">
        <allow-intent href="market:*"/>
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*"/>
        <allow-intent href="itms-apps:*"/>
    </platform>
    <engine name="android" spec="~7.1.4"/>
</widget>